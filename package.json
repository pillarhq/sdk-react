{
  "name": "@pillar-ai/react",
  "version": "0.1.4",
  "description": "React bindings for Pillar Embedded Help SDK",
  "type": "module",
  "main": "./dist/index.esm.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "default": "./dist/index.esm.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "dev": "rollup -c -w",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "help",
    "documentation",
    "support",
    "chat",
    "ai"
  ],
  "author": "Pillar Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pillarhq/sdk-react",
    "directory": "packages/sdk-react"
  },
  "homepage": "https://trypillar.com",
  "bugs": {
    "url": "https://github.com/pillarhq/sdk-react/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">=17.0.0 || >=19.0.0",
    "react-dom": ">=17.0.0 || >=19.0.0"
  },
  "dependencies": {
    "@pillar-ai/sdk": "^0.1.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rollup": "^4.9.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.0"
  },
  "sideEffects": false
}
